{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Agent Turns (rate/min)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(nullclaw_requests_total[5m]) * 60",
          "legendFormat": "requests/min"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "fillOpacity": 15, "lineWidth": 2 }
        },
        "overrides": []
      }
    },
    {
      "title": "Error Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(nullclaw_errors_total[5m]) * 60",
          "legendFormat": "errors/min"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "red" },
          "custom": { "fillOpacity": 15, "lineWidth": 2 }
        },
        "overrides": []
      }
    },
    {
      "title": "LLM Request Latency (p95)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(nullclaw_duration_milliseconds_bucket{span_name=\"llm_response\"}[5m])) by (le))",
          "legendFormat": "p95 latency (ms)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": { "fillOpacity": 10, "lineWidth": 2 }
        },
        "overrides": []
      }
    },
    {
      "title": "Tool Calls by Name",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum by (span_name) (rate(nullclaw_calls_total{span_name=~\"tool_call.*\"}[5m])) * 60",
          "legendFormat": "{{span_name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "fillOpacity": 15, "lineWidth": 2 }
        },
        "overrides": []
      }
    },
    {
      "title": "Recent Traces",
      "type": "traces",
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 16 },
      "datasource": { "type": "tempo", "uid": "tempo" },
      "targets": [
        {
          "queryType": "traceqlSearch",
          "limit": 20,
          "filters": [
            { "id": "service-name", "tag": "service.name", "operator": "=", "value": ["nullclaw"], "scope": "resource" }
          ]
        }
      ]
    }
  ],
  "refresh": "10s",
  "schemaVersion": 39,
  "tags": ["nullclaw", "observability"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "nullclaw Agent Overview",
  "uid": "nullclaw-overview"
}
